{
    "name": "laravolt/votee",
    "description": "Upvote downvote for any eloquent object",
    "keywords": [
        "laravolt",
        "votee",
        "upvote",
        "downvote",
        "like",
        "dislike"
    ],
    "homepage": "https://github.com/laravolt/votee",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Votee\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Votee\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
